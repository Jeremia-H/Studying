public class Kata 
{
    public static int FindSmallestInt(int[] args) 
    {
        for (int i = 0; i < args.Length-1; i++)
            {
                if (args[i] < args[i + 1])
                {
                    args[i + 1] = args[i];
                }
            }
            return args[args.Length-1];
    }
}
